 * Restores the symbol referenced by DashboardScreen.kt. Replace with
 * full UI later as needed.
 */
@Composable
fun MarketOverviewTab(selectedPair: ForexPair, onAssetClick: (ForexPair) -> Unit = {}) {
    Column(modifier = Modifier
        .fillMaxSize()
        .padding(16.dp)) {
        Text(text = "Market Overview", color = Color.White)
        Spacer(modifier = Modifier.height(8.dp))
        Text(text = "Selected: ${selectedPair.symbol}", color = Color.White)
